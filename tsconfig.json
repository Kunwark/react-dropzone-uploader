{
  "compilerOptions": {
    "target": "ESNext",
    "esModuleInterop": true,
    "jsx": "react",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false
  },
  "include": ["src/*.tsx", "src/*.ts"],
  "exclude": ["node_modules"]
}
